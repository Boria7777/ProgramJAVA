package com.bao.test;

public class Zhishu {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int i = 2; i <= 1000; i++) { // ÖÊÊý
            for (int k = 2; k <= i; k++) { // ³ýÊý
                
                if (i % k == 0 && i != k) {
                    break;
                }
                
                if (i % k == 0 && i == k) {
                    System.out.println(i);
                }
            }
        }
	 }
  }


